<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <path
                android:name="path"
                android:pathData="M 9 17.3 L 12 20.3 L 15 17.3 C 13.3 15.6 10.7 15.6 9 17.3 Z"
                android:fillColor="#000"
                android:strokeWidth="1"/>
            <path
                android:name="path_1"
                android:pathData="M 5 13.3 L 7 15.3 C 9.8 12.5 14.2 12.5 17 15.3 L 19 13.3 C 15.1 9.4 8.9 9.4 5 13.3 Z"
                android:fillColor="#000"
                android:fillAlpha="0"
                android:strokeWidth="1"/>
            <path
                android:name="path_2"
                android:pathData="M 1 9.3 L 3 11.3 C 8 6.3 16 6.3 21 11.3 L 23 9.3 C 16.9 3.2 7.1 3.2 1 9.3 Z"
                android:fillColor="#000"
                android:fillAlpha="0"
                android:strokeWidth="1"/>
        </vector>
    </aapt:attr>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:duration="480"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/linear_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="path_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:startOffset="330"
                android:duration="520"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/linear_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="path_2">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:startOffset="660"
                android:duration="340"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:anim/linear_interpolator"/>
        </aapt:attr>
    </target>
</animated-vector>
